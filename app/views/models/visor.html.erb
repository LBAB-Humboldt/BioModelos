
<% flash.each do |key, value| %>
  <div class="alert alert-<%= key %>"><%= value %></div>
<% end %>
<h1>Modelos de Distribuci√≥n de Especies</h1>
<%= render 'search_form' %>
<div id="btnPanel">
	<button type="button" id="editBtn" class="btn">Edicion</button><br />
