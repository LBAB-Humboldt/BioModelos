.formasignin {
	background-color: #FAC205;
	font-size: 12px;
	width: 200px;
	height: 100px;
	padding: 30px 18px 10px 15px;
	border-bottom: 5px solid #FAE49B;
  background-image: url(<%= asset_path 'arroww.png' %>);
  background-repeat: no-repeat;
  background-position: 190px top;
  float: right;
  margin-right: 130px;
}

.bbton {
	padding: 1px 10px;
	background-color: #76CCC5;
	color:#fff;
	font-family: 'bebas_neueregular', sans-serif;
  text-transform: uppercase;
  font-size: 1.2em;
  letter-spacing: 1.2px;
  border:none;
  float: right;
}

.inputforma {
	width: 190px;
	margin-bottom: 10px;
	border:none;
	padding: 3px;
	border-right: 5px solid #FAE49B;
}

.checkbox {
	float: left;
  vertical-align: middle;
  font-size: 10px;
  color:#fff;
  letter-spacing: .8px;
  margin-top: 3px;
  margin-right: 5px;
}

.checkrec {
}

.linkss {
  text-decoration: none;
  color:#008D96;
  font-size: 10px;
  display: inline-block;
  margin: 10px;
  text-align: center;
}

::-webkit-input-placeholder {
   color: #A3A3A3;
   font-style: italic;
   font-size: 10px;
}

:-moz-placeholder { /* Firefox 18- */
   color: #A3A3A3;
   font-style: italic;
   font-size: 10px;  
}

::-moz-placeholder {  /* Firefox 19+ */
   color: #A3A3A3;
   font-style: italic;
   font-size: 10px;  
}

:-ms-input-placeholder {  
   color: #A3A3A3;
   font-style: italic;
   font-size: 10px;  
}