.maincol{
    width: 225px;
    padding-right: 25px;
    margin:20px 20px 20px 40px;
    border-right: 1px dashed #76CCC5;
    position: absolute;
    height: 100%;
}

.BioM {
    width: 205px;
    border: 1px solid #008D96;
    border-bottom: 5px solid #008D96;
    padding: 10px;
    line-height: 1;
    margin-bottom: 5px;
}

.BioM h1 {
    color: #5D5D5D;
}

.BioM p {
    font-size: 12px;
    font-weight: 700;
    color: #008D96;
}

.intro {
    padding: 5px;
    font-size: 12px;
    color: #5D5D5D;
    font-weight: 700 !important;
}

.intro strong {
    color: #008D96;
}

.opciones {
	padding-left: 285px;
}

.lineop {
	width: 100%;
	clear: both;
}

.op {
	margin:20px 20px;
	display: inline-block;
}

.op:last-child{
	margin-right: 0;
}

.op h2 {
	width: 145px;
	height: 125px;
	font-size: 36px;
	color:#FAC205;
	text-align: left;
	background-position: right bottom;
	background-repeat: no-repeat;
}

.op p {
	text-align: center;
	font-size: 11px;
	color: #008D96;
	margin-top: 5px;
	margin-left: 20px;
}

.ingresa {
	background-image: url(<%= asset_path 'ingresa.png' %>);
}

.selecciona {
	background-image: url(<%= asset_path 'selecciona.png' %>);
}

.consulta {
	background-image: url(<%= asset_path 'consulta.png' %>);
}

.edita {
	background-image: url(<%= asset_path 'edita.png' %>);
}

.comenta {
	background-image: url(<%= asset_path 'comenta.png' %>);
}

.red {
	background-image: url(<%= asset_path 'red.png' %>);
}

	