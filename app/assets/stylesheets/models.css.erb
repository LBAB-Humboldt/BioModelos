#map {
	padding-top: 89px;
	margin-top: -89px;
	width: 100%;
	height: -webkit-calc(100% - 90px);
	height: -moz-calc(100% - 90px);
	height: calc(100% - 90px);
}

.selectores {
	width: 100%;
    position: absolute;
    bottom: 120px;
    z-index:100;
    text-align: center;
    margin: 0 auto;
}

.selectores a img {
	width: 10%;
	display: inline-block;
	margin: 1%;
}

.titulomapa {
	width: 130px;
	margin: 5px auto;
    padding: 3px;
    font-size: 12px;
    background-color: #fff;
    color:#e48413;
    z-index:100;
}

.findbar {
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 16px;
	line-height: 1;
	letter-spacing: 1px;
	text-align: center;
    padding: 5px 35px 5px 10px;
    background-color: #e48413;
    position: absolute;
    min-width: 155px;
    top: 227px;
	right:-88px;   
	color:#fff;
    z-index:110;
    -webkit-transform: rotate(-90deg);
    -moz-transform: rotate(-90deg);
    -ms-transform: rotate(-90deg);
    -o-transform: rotate(-90deg);
    transform: rotate(-90deg);
    background-image: url(<%= asset_path 'lupa.png' %>);
    background-repeat: no-repeat;
    background-position: right;
}

.cajabusqueda {
	width: 458px;
	height: 175px;
	padding: 5px 20px 20px;
	border-left: 5px solid #F9A848;
	position: absolute;
	top: 140px;
	right: 25px;
	z-index: 120;
	background: rgb(232, 139, 20); 
	background: rgba(232, 139, 20, 0.7);
}

#search_field {
	width: 220px;
	height: 14px;
	border: none;
	padding: 3px;
	color:#5d5d5d;
}

.searchbtn {
	width: 20px;
	height: 20px;
	background-color:#1ab0b5;
	background-image: url(<%= asset_path 'lupabtn.png' %>);
	border: none;
	vertical-align: bottom;
	margin-left: -3px;
	cursor: pointer;
	display: inline-block;
}

.cajabusqueda p {
	line-height: 1;
	font-size: 11px;
	color:#fff;
	list-style: 1;
	margin-bottom: 10px;
	margin-top: 5px;
}

.searchcateg {
	float: right;
	padding:15px 65px 15px 0;
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 23px;
	font-weight: 400;
	letter-spacing: 2px;
	color:#3d3d3d;
	vertical-align: middle;
	background-position: top right;
	-webkit-background-size: contain;
	background-size: contain;
	background-repeat: no-repeat;
}

.srchaves {
	background-image: url(<%= asset_path 'avesg.png' %>);
}
.srchinvertebrados {
	background-image: url(<%= asset_path 'invertebradosg.png' %>);
}
.srchanfibios {
	background-image: url(<%= asset_path 'anfibiosg.png' %>);
}
.srchmamiferos {
	background-image: url(<%= asset_path 'mamiferosg.png' %>);
}
.srchplantas {
	background-image: url(<%= asset_path 'plantasg.png' %>);
}
.srchreptiles {
	background-image: url(<%= asset_path 'reptilesg.png' %>);
}
.srchpeces {
	background-image: url(<%= asset_path 'pecesg.png' %>);
}

.resultados {
	width: 100%;
	height: 105px;
	background-color: #fff;
	padding: 5px 5px 0;
}

.mbton {
	position: absolute;
	text-align: center;
	bottom: 0;
	right: 0;
	padding: 1px 10px;
	background-color: #cde31e;
	color:#fff;
	font-family: 'bebas_neueregular', sans-serif;
  	text-transform: uppercase;
  	font-size: 1em;
  	letter-spacing: 2px;
  	border:none;
  	float: right;
  	cursor:pointer;
  	margin: 13px 10px;
}

.resbusqueda {
	float: left;
	font-size: 12px;
	line-height: 1.2;
}

.familia {
	color: #adc010;
	font-weight: 700;
}

.genero {
	color: #3d3d3d;
	font-weight: 700;
}

.especie {
	color: #5d5d5d;
}

.resbusqueda a {
	font-size: 12px;
	line-height: 3;
	color: #1ab0b5;
}

.comun {
	color: #5d5d5d;
}

.comun strong {
	color: #3d3d3d;
	font-weight: 700;
}

.registros {
	float: right;
	text-align: right;
	font-size: 12px;
	line-height: 1.2;
}

.registros h4 {
	font-weight: 700;
	color: #FAC205;
}

.noreg {
	font-weight: 700;
	color: #3d3d3d;
}

.showmodels {
	width: 80%;
	height: -webkit-calc(100% - 167px);
	height: -moz-calc(100% - 167px);
	height: calc(100% - 167px);
	position: absolute;
	top: 83px;
	left:10%;
	z-index: 120;
	background-image: url(<%= asset_path 'fondobla.jpg' %>);
	overflow:auto;
}

.showmodels h1 {
	padding: 15px 10px 5px;
	background-color:#e7cb7f;
	font-family: 'bebas_neueregular', sans-serif;
	letter-spacing: 2px;
	color: #30302E;
}

.showmodels p {
	font-size: 11px;
	padding-left: 10px;
	letter-spacing: 1px;
	color: #5D5D5D;
	margin-bottom: 10px;
}

.padmodelos {
	margin-top: 10px;
	padding: 10px;
	width: 80%;
	margin-bottom: 20px;
	overflow:auto;
}

.commentspace h3 {
	margin-left: 10px;
	color:#81e0e3;
	text-transform: uppercase;
}

.commeinput {
	width: 60%;
	padding: 3px;
	margin: 10px 10px 0;
	border: 1px dashed #81e0e3;
	font-family: 'Lato', sans-serif !important;
    color: #414243;
    font-style: italic;
    font-size: 12px;
    letter-spacing: 1px;
}

.cbton {
	padding:0 10px;
	margin-left: 5px;
	background-color: #FFC717;
	color:#fff;
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 1em;
	letter-spacing: 2px;
	border:none;
	cursor:pointer;
	vertical-align: top;
}
.modelothumb {
	display: inline-block;
	margin: 10px;
}

.modelothumb h3 {
	display: inline-block;
	padding: 1px 5px;
	color:#fff;
	background-color: #FFC717;
	border-bottom: 3px solid #FAE49B;
	vertical-align: top;
}

.modelothumb img {
	border: 1px solid #FFC717;
	margin-left: -25px;
	padding: 5px;
}

.editbtn {
	padding: 1px 5px;
	display: inline-block;
	margin: 5px 10px;
	background-color: #3d3d3d;
	color: #fff;
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 1em;
	letter-spacing: 2px;
	cursor: pointer;
}

.rankmod {
	border-bottom: 1px dashed #FAE49B;
}

.rankmod img {
	float:left;
	margin: 5px 5px 5px 0;
	padding: 0;
	border:none;
}

